{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Describes an instructor for a given section",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "description": "UUID for this instructor",
      "$ref": "uuid.json"
    },
    "name": {
      "type": "string",
      "description": "The name of this instructor"
    }
  },
  "required": [
    "name"
  ]
}
